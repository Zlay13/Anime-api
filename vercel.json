{
  "name": "otakudesu-api",
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "index.js" }
  ],
  "env": {
    "EXAMPLE_ENV_VARIABLE": "example-value" // Tambahkan variabel lingkungan yang diperlukan di sini
  }
}
